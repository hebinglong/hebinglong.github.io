<% if (site.posts.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">
        <i class="icon-library vm"></i>
        <span class="vm"><%= __('archive_a') %></span>
    </h3>
    <div class="widget">
      <%- list_archives({format: 'Y/M', show_count: theme.show_count, type: theme.archive_type}) %>
    </div>
  </div>
<% } %>
